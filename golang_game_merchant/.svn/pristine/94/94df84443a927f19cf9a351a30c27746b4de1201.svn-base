package model

//表： merchant_warn_log
type MerchantWarnLog struct {
	UserName   string
	Content    string
	CreateTime int64
	IP         string
	Device     int
}

func QueryWarningLog(mid int) ([]MerchantWarnLog, error) {
	var m []MerchantWarnLog
	if err := Db.Where("merchant_id = ?", mid).Find(&m).Error; err != nil {
		return nil, err
	}
	return m, nil
}
